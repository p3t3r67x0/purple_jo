# DNS Record Extraction Services Configuration
# Copy this file to .env and customize as needed

# Database Configuration - Connect to your existing PostgreSQL instance
# Update with your actual database credentials and database name
POSTGRES_DSN=postgresql+asyncpg://your_user:your_password@localhost:5432/netscanner

# RabbitMQ Configuration - Connect to your existing RabbitMQ instance
RABBITMQ_URL=amqp://guest:guest@localhost:5672/

# DNS Service Configuration
DNS_QUEUE_NAME=dns_records
DNS_CONCURRENCY=500
DNS_PREFETCH=100
DNS_TIMEOUT=1.0
DNS_BATCH_SIZE=5000

# Worker Configuration
DEFAULT_WORKERS=4
WORKER_MEMORY_LIMIT=512M
WORKER_MEMORY_RESERVATION=256M

# Monitoring (Access existing services on host)
RABBITMQ_MANAGEMENT_URL=http://localhost:15672
POSTGRES_HOST=localhost
POSTGRES_PORT=5432

# Logging
LOG_LEVEL=INFO
VERBOSE_LOGGING=false