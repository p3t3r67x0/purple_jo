# Example configuration for crawl_urls_postgres.service
POSTGRES_DSN=postgresql+asyncpg://user:pass@db-host:5432/netscanner
RABBITMQ_URL=amqp://guest:guest@rabbitmq:5672/
QUEUE_NAME=crawl_domains
WORKER_PROCESSES=4
CONCURRENCY=500
PREFETCH=10
REQUEST_TIMEOUT=10.0
CONNECT_TIMEOUT=5.0
MAX_REDIRECTS=5
MAX_RETRIES=2
SCHEMES=https,http
# Additional CLI flags, e.g. '--verbose-urls'
EXTRA_ARGS=
PUBLISHER_WORKER_COUNT=8
PUBLISHER_BATCH_SIZE=10000
PUBLISHER_PURGE_FLAG=--purge-queue
PUBLISHER_EXTRA_ARGS=
